
@model Officer



<!-- Spinner Start -->
<div id="spinner" class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
    <div class="spinner-grow text-primary" style="width: 3rem; height: 3rem;" role="status">
        <span class="sr-only">Loading...</span>
    </div>
</div>
<!-- Spinner End -->
<!-- Topbar Start -->
<div class="container-fluid bg-light p-0 wow fadeIn" data-wow-delay="0.1s">
    <div class="row gx-0 d-none d-lg-flex">
        <div class="col-lg-7 px-5 text-start">
            <div class="h-100 d-inline-flex align-items-center py-3 me-4">
                <small class="fa fa-map-marker-alt text-primary me-2"></small>
                <small>Plot No-742/2784,Bharat Villa, Rudrapur,Pin-752101</small>
            </div>
            <div class="h-100 d-inline-flex align-items-center py-3">
                <small class="far fa-clock text-primary me-2"></small>
                <small>24 x 7</small>
            </div>
        </div>
        <div class="col-lg-5 px-5 text-end">
            <div class="h-100 d-inline-flex align-items-center py-3 me-4">
                <small class="fa fa-phone-alt text-primary me-2"></small>
                <small>9337232208</small>
            </div>
            <div class="h-100 d-inline-flex align-items-center">
                <a class="btn btn-sm-square rounded-circle bg-white text-primary me-1" href=""><i class="fab fa-facebook-f"></i></a>
                <a class="btn btn-sm-square rounded-circle bg-white text-primary me-1" href=""><i class="fab fa-twitter"></i></a>
                <a class="btn btn-sm-square rounded-circle bg-white text-primary me-1" href=""><i class="fab fa-linkedin-in"></i></a>
                <a class="btn btn-sm-square rounded-circle bg-white text-primary me-0" href=""><i class="fab fa-instagram"></i></a>
            </div>
        </div>
    </div>
</div>
<!-- Topbar End -->
<!-- Navbar Start -->
<nav class="navbar navbar-expand-lg bg-white navbar-light sticky-top p-0 wow fadeIn" data-wow-delay="0.1s">
    <a href="index.html" class="navbar-brand d-flex align-items-center px-4 px-lg-5">
        <h1 class="m-0 text-primary"><i class="far fa-hospital me-3"></i>LABOLOGY</h1>
    </a>
    <button type="button" class="navbar-toggler me-4" data-bs-toggle="collapse" data-bs-target="#navbarCollapse">
        <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarCollapse">
        <div class="navbar-nav ms-auto p-4 p-lg-0">
            <ul class="navbar-nav me-auto">
                <li class="nav-item">
                    <a class="nav-item nav-link " asp-area="Customer" asp-controller="Home" asp-action="Index">Home</a>
                </li>
                <li class="nav-item">
                    <a class="nav-item nav-link " asp-area="Customer" asp-controller="Home" asp-action="AboutUs">About Us</a>
                </li>
                <li class="nav-item">
                    <a class="nav-item nav-link" asp-area="Customer" asp-controller="Home" asp-action="Services">Services</a>
                </li>


                @if (User.IsInRole(SD.Role_Admin))
                {
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle active" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                            Content Manegment
                        </a>
                        <ul class="dropdown-menu">
                            <li class="nav-item">
                                <a class="dropdown-item active" asp-area="Admin" asp-controller="Officer" asp-action="Index">Officer</a>
                            </li>

                            <li class="nav-item">
                                <a class="dropdown-item" asp-area="Admin" asp-controller="Client" asp-action="Index">Clints</a>
                            </li>

                            <li class="nav-item">
                                <a class="dropdown-item" asp-area="Customer" asp-controller="Home" asp-action="Appointment">Appointment</a>
                            </li>

                            <li><hr class="dropdown-divider" /></li>

                            <li class="nav-item">
                                <a class="dropdown-item" asp-area="Identity" asp-page="/Account/Register">Create User</a>
                            </li>

                            <li class="nav-item">
                                <a class="dropdown-item" asp-area="Admin" asp-controller="User" asp-action="Index">User</a>
                            </li>

                        </ul>
                    </li>
                }
                @if (User.IsInRole(SD.Role_Officer))
                {
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                            Content Manegment
                        </a>
                        <ul class="dropdown-menu">

                            <li class="nav-item">
                                <a class="dropdown-item" asp-area="Admin" asp-controller="Client" asp-action="Index">Clints</a>
                            </li>

                        </ul>
                    </li>
                }
            </ul>

        </div>
        <partial name="_LoginPartial" />
    </div>
</nav>
<!-- Navbar End -->

<div class="card shadow border-0 m-4">
    <div class="card-header bg-secondary bg-gradient ml-0 py-3">
        <div class="row">
            <div class="col-12 text-center">
                <h2 class="text-white">Delete Officer</h2>
            </div>
        </div>
    </div>
    <div class=" card-body p-4">
        <form method="post">
            <input asp-for="Id" hidden />

            <div class="border p-3">


                <div class="form-floating py-2 col-12">
                    <input asp-for="Name" disabled class="form-control border-0 shadow" />
                    <label asp-for="Name" class="ms-2"></label>

                </div>
                <div class="form-floating py-2 col-12">
                    <input asp-for="Mobile" disabled class="form-control border-0 shadow" />
                    <label asp-for="Mobile" class="ms-2 text-dark"></label>

                </div>
                <div class="form-floating py-2 col-12">
                    <input asp-for="Dist" disabled class="form-control border-0 shadow" />
                    <label asp-for="Dist" class="ms-2 text-dark"></label>

                </div>
                <div class="form-floating py-2 col-12">
                    <input asp-for="Pos" disabled class="form-control border-0 shadow" />
                    <label asp-for="Pos" class="ms-2 text-dark"></label>

                </div>
                <div class="form-floating py-2 col-12">
                    <input asp-for="Pin" disabled class="form-control border-0 shadow" />
                    <label asp-for="Pin" class="ms-2 text-dark"></label>

                </div>
                <div class="form-floating py-2 col-12">
                    <textarea asp-for="LandMark" disabled class="form-control border-0 shadow"></textarea>
                    <label asp-for="LandMark" class="ms-2 text-dark"></label>

                </div>
                <div class="form-floating py-2 col-12">
                    <textarea asp-for="Totalclient" disabled class="form-control border-0 shadow"></textarea>
                    <label asp-for="Totalclient" class="ms-2 text-dark"></label>

                </div>
                <div class="row mt-3">
                    <div class="col-6 col-md-3">
                        <button type="submit" class="btn btn-danger form-control">Delete</button>
                    </div>
                    <div class=" col-6 col-md-3">
                        <a asp-controller="Officer" asp-action="Index" class="btn btn-secondary border form-control">
                            Back To List
                        </a>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>




